{
  "name": "@kong-ui/demo-component",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/demo-component.umd.js",
  "module": "./dist/demo-component.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/demo-component.js",
      "require": "./dist/demo-component.umd.cjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "dev": "cross-env DEV_SANDBOX=true vite",
    "build": "run-s typecheck build:package build:types",
    "build:package": "vite build",
    "build:types": "vue-tsc --emitDeclarationOnly",
    "preview": "vite preview --port 4173",
    "lint": "eslint '**/*.{js,jsx,ts,tsx,vue}' --ignore-path ../../.eslintignore",
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx,vue}' --ignore-path ../../.eslintignore --fix",
    "typecheck": "vue-tsc --noEmit"
  },
  "repository": "https://github.com/Kong/kong-ui-shared-components/tree/main/packages/demo-component",
  "author": "Kong, Inc.",
  "license": "UNLICENSED",
  "volta": {
    "extends": "../../package.json"
  },
  "peerDependencies": {
    "vue": "^3.2.38"
  },
  "dependencies": {
    "@kong-ui/utils": "^0.0.1"
  }
}
